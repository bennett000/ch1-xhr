{
    "name"        : "<%= target %>-<%= pkg.name %>",
    "version"     : "<%= pkg.version %>",
    "description" : "<%= pkg.description %>",
    "moduleType"  : [
        "globals"
    ],
    "main": ["http-client.min.js", "http-client.min.js.map", "http-client.js"],
    "dependencies": {
        "<%= target %>":"<%= targetSrc %>"
    },
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "lib",
        "test",
        "tests"
    ]
}
